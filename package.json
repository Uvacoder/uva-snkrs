{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "my sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "n/a",
  "prettier": "@mcansh/eslint-config/prettier.config",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run generate"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "prisma generate",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "jest",
    "type-check": "tsc",
    "validate": "npm-run-all --parallel lint type-check test",
    "db:save": "prisma migrate save --experimental",
    "db:up": "prisma migrate up --experimental",
    "db:generate": "prisma generate",
    "db:migrate": "npm-run-all db:*",
    "find:unused": "next-unused",
    "postinstall": "npm run generate"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src"
    ],
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "@prisma/client": "2.12.1",
    "@svgr/webpack": "5.5.0",
    "argon2": "0.27.0",
    "babel-plugin-superjson-next": "0.1.8",
    "cloudinary-core": "2.11.3",
    "clsx": "1.1.1",
    "date-fns": "2.16.1",
    "dequal": "2.0.2",
    "fathom-client": "3.0.0",
    "formik": "2.2.5",
    "next": "10.0.3",
    "next-iron-session": "4.1.10",
    "next-offline": "5.0.3",
    "next-seo": "4.17.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "superjson": "1.4.1",
    "swr": "0.3.9",
    "type-fest": "0.20.1",
    "yup": "0.31.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@mcansh/eslint-config": "3.0.1",
    "@next/eslint-plugin-next": "10.0.3",
    "@prisma/cli": "2.12.1",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/argon2": "0.15.0",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/yup": "0.29.9",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "autoprefixer": "10.0.2",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-kentcdodds": "17.1.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.10.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.2",
    "next-unused": "0.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.1.10",
    "prettier": "2.2.0",
    "shell-quote": "1.7.2",
    "tailwindcss": "2.0.1",
    "typescript": "4.1.2"
  }
}
