{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "show off your sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "remix.config.js",
  "sideEffects": false,
  "scripts": {
    "dev": "pm2-dev pm2.config.js",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:icons": "node scripts/heroicon-symbols.mjs",
    "build:remix": "npm run generate:routes && remix build",
    "generate:db": "prisma generate",
    "generate:routes": "routes-gen -d @routes-gen/remix",
    "generate:global": "gen-env-types .env -o app/@types/global.d.ts -e .",
    "generate": "run-p generate:*",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx,.mjs,.cjs,.cts,.mts .",
    "test": "jest --passWithNoTests",
    "type-check": "tsc",
    "validate": "run-p lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "prepare": "husky install",
    "clean": "del .cache build public/build app/styles app/icons/{solid,outline}",
    "start": "cross-env NODE_ENV=production node ./build",
    "deploy": "flyctl deploy"
  },
  "dependencies": {
    "@headlessui/react": "1.6.0",
    "@mcansh/remix-secure-headers": "0.5.0",
    "@prisma/client": "3.13.0",
    "@reach/alert": "0.17.0",
    "@remix-run/express": "1.4.1",
    "@remix-run/node": "1.4.1",
    "@remix-run/react": "1.4.1",
    "@remix-run/serve": "1.4.1",
    "@sentry/node": "6.19.7",
    "accounting": "0.4.1",
    "cloudinary": "1.29.1",
    "cloudinary-build-url": "0.2.4",
    "clsx": "1.1.1",
    "cross-env": "7.0.3",
    "csv-string": "4.1.0",
    "cuid": "2.1.8",
    "date-fns": "2.28.0",
    "etag": "1.8.1",
    "express": "4.18.0",
    "fathom-client": "3.4.1",
    "lodash.uniqby": "4.7.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-number-format": "4.9.3",
    "remix-flat-routes": "0.3.0",
    "remix-seo": "0.1.0",
    "routes-gen": "0.4.0",
    "slugify": "1.6.5",
    "tiny-invariant": "1.2.0",
    "type-fest": "2.12.2",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@cld-apis/types": "0.1.6",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@mcansh/eslint-config": "4.0.0",
    "@remix-run/dev": "1.4.1",
    "@remix-run/eslint-config": "1.4.1",
    "@routes-gen/remix": "0.3.0",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@types/accounting": "0.4.2",
    "@types/etag": "1.8.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/lodash.uniqby": "4.7.7",
    "@types/node": "17.0.29",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.2",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "autoprefixer": "10.4.5",
    "cssnano": "5.1.7",
    "del-cli": "4.0.1",
    "dotenv": "16.0.0",
    "eslint": "8.14.0",
    "gen-env-types": "1.3.4",
    "heroicons": "1.0.6",
    "husky": "7.0.4",
    "jest": "28.0.2",
    "lint-staged": "12.4.1",
    "npm-run-all": "4.1.5",
    "pm2": "5.2.0",
    "postcss": "8.4.12",
    "postcss-100vh-fix": "1.0.2",
    "postcss-cli": "9.1.0",
    "prettier": "2.6.2",
    "prisma": "3.13.0",
    "shell-quote": "1.7.3",
    "svgo": "2.8.0",
    "tailwindcss": "3.0.24",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  }
}
