{
  "name": "@mcansh/snkrs",
  "version": "1.0.0",
  "description": "show off your sneaker collection",
  "repository": "mcansh/snkrs",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "remix.config.js",
  "prettier": "@mcansh/eslint-config/prettier.config",
  "sideEffects": false,
  "scripts": {
    "dev": "pm2-dev pm2.config.js",
    "prebuild": "npm run clean",
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:icons": "node scripts/heroicon-symbols.mjs",
    "build:remix": "remix build",
    "generate": "prisma generate",
    "gen:types": "gen-env-types .env -o app/@types/global.d.ts -e .",
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "test": "jest --passWithNoTests",
    "type-check": "tsc",
    "validate": "npm-run-all --parallel lint type-check test",
    "db:migrate": "prisma migrate dev --preview-feature",
    "prepare": "husky install && remix setup && metronome setup && npm run generate",
    "clean": "del .cache build public/build app/styles app/icons/{solid,outline}",
    "start": "remix-serve ./build"
  },
  "dependencies": {
    "@headlessui/react": "1.4.1",
    "@metronome-sh/node": "1.0.5",
    "@metronome-sh/react": "1.0.5",
    "@prisma/client": "3.0.2",
    "@remix-run/node": "0.18.2",
    "@remix-run/react": "0.18.2",
    "@remix-run/serve": "0.18.2",
    "accounting": "0.4.1",
    "cloudinary": "1.27.0",
    "cloudinary-core": "2.11.4",
    "clsx": "1.1.1",
    "cuid": "2.1.8",
    "date-fns": "2.24.0",
    "etag": "1.8.1",
    "express": "4.17.1",
    "fathom-client": "3.2.0",
    "framer-motion": "4.1.17",
    "ioredis": "4.27.9",
    "lodash.uniqby": "4.7.0",
    "react": "18.0.0-alpha-baff3f200-20210921",
    "react-dom": "18.0.0-alpha-baff3f200-20210921",
    "react-hot-toast": "2.1.1",
    "react-number-format": "4.7.3",
    "react-router-dom": "6.0.0-beta.3",
    "remix": "0.18.2",
    "remix-utils": "1.0.2",
    "reserved-email-addresses-list": "0.0.9",
    "slugify": "1.6.0",
    "type-fest": "2.3.4",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@mcansh/eslint-config": "3.1.0",
    "@remix-run/dev": "0.18.2",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/forms": "0.3.3",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.0",
    "@types/accounting": "0.4.2",
    "@types/etag": "1.8.1",
    "@types/ioredis": "4.27.4",
    "@types/jest": "27.0.1",
    "@types/lodash.uniqby": "4.7.6",
    "@types/node": "16.9.4",
    "@types/react": "17.0.22",
    "@types/react-dom": "17.0.9",
    "@types/yup": "0.29.13",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "autoprefixer": "10.3.4",
    "cssnano": "5.0.8",
    "del-cli": "4.0.1",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "gen-env-types": "1.3.0",
    "heroicons": "1.0.4",
    "husky": "7.0.2",
    "jest": "27.2.1",
    "lint-staged": "11.1.2",
    "make-dir": "3.1.0",
    "npm-run-all": "4.1.5",
    "pm2": "5.1.1",
    "postcss": "8.3.6",
    "postcss-100vh-fix": "1.0.2",
    "postcss-cli": "8.3.1",
    "prettier": "2.4.1",
    "prisma": "3.0.2",
    "shell-quote": "1.7.2",
    "svgo": "2.6.1",
    "tailwindcss": "2.2.15",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  }
}
